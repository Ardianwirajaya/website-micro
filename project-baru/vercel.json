{
  "version": 2,
  "functions": {
    "api/auth/index.js": { "runtime": "vercel-node@latest" },
    "api/products/index.py": { "runtime": "vercel-python@latest" },
    "api/orders/main.go": { "runtime": "vercel-go@latest" }
  },
  "routes": [
    { "src": "/api/auth", "dest": "/api/auth/index.js" },
    { "src": "/api/products", "dest": "/api/products/index.py" },
    { "src": "/api/orders", "dest": "/api/orders/main.go" },
    { "src": "/(.*)", "dest": "/frontend/$1" }
  ]
}